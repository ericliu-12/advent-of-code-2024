    "import re\n",
    "answer = 0\n",
    "with open(\"data.txt\") as file:\n",
    "    content = file.read()\n",
    "correct = re.findall(\"mul\\([0-9]+,[0-9]+\\)\", content)\n",
    "for fun in correct:\n",
    "    x, y = fun[4:-1].split(\",\")\n",
    "    answer += int(x) * int(y)\n",
    "print(answer)\n",
    "\n",
    "\n"
